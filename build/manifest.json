{
  "manifest_version": 3,
  "name": "Svelte Component Explorer",
  "version": "1.0.0",
  "icons": {
    "16": "icons/tuner.png",
    "48": "icons/tuner.png",
    "128": "icons/tuner.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "devtools_page": "devtools.html",
  "background": {
    "service_worker": "background.js",
    "type": "module",
    "minimum_chrome_version": "92"
  },
  "content_scripts": [
    {
      "matches": [
        "http://localhost/*"
      ],
      "js": [
        "import-contentScript.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "http://localhost/*"
      ],
      "js": [
        "import-contentScriptIsolated.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "chunks/*-*.js",
        "contentScript.js",
        "contentScriptIsolated.js"
      ],
      "matches": [
        "http://localhost/*"
      ]
    }
  ]
}